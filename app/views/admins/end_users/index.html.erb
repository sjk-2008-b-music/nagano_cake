<div class="body">
  <div class="container">
    <h3>会員一覧</h3>
    <table class="table table-inverse panel panel-default">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>

      <tbody>
        <% @end_users.each do |end_user| %>
          <tr>
            <td>
              <%= end_user.id %>
            </td>
            <td>
              <%= link_to admins_end_user_path(end_user) do %>
                <%= end_user.last_name+end_user.first_name %>
                  <% end %>
            </td>
            <td>
              <%= end_user.email %>
            </td>
            <td>
              <% if end_user.is_deleted %>
                退会済
              <% else %>
                有効
              <% end %>
            </td>
          </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>
